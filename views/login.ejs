<!doctype html>
<head>
	<% include head.ejs %>
</head>
<body>
	<%= message %>
	<form class="form-horizontal" action="/users/login_receiver" enctype="multipart/form-data" method="post">
		<div class="form-group">
			<label for="name" class="col-sm-2 control-label">Store Name</label>
			<div class="col-sm-10">
				<input type="text" name="given_name" id="name" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="password" class="col-sm-2 control-label">Password</label>
			<div class="col-sm-10">
				<input type="text" name="given_pass" id="password" class="form-control">
			</div>
		</div>
		<div class="form-group" class="col-sm-2 control-label">
			<input class="btn btn-info" type="submit" value="login" name="push_button">
		</div>
	</form>
</body>
</html>