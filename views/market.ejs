<!doctype html>

<head>
	<% include head.ejs %>
</head>

<body>
<% include partial_market_navbar %>
<% include partial_market.ejs %>

<div>
	<% for(var i=0; i < data.length; i++) { %>
	    <div class="col-sm-6 col-md-3" id="shop_div">

	    	<div style="height:290px" class="thumbnail">
	   			<img src="/images/<%=data[i].profile_pic%>">
	   		</div>

	   		<div class="caption">
		    	<h3><%= data[i].name %></h3>
		    </div>

		    <form action="/store" method=post enctype="multipart/form-data">
			    <div class="form-group">
	    			<div class="col-sm-offset-2 col-sm-10">
	    				<input type="hidden" name="store_name" value="<%= data[i].store_name %>">
	    				<input type="submit" value="Visit" class="btn btn-info">
	    			</div>
	    		</div>
			</form>
	   </div>
	<% } %>
</div> 

</body>
</html>
