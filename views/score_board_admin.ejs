<!DOCTYPE html>
<html>
<head>
	<% include partial_head.ejs %>
</head>
<body>
	<% include partial_navbar.ejs %>

	<form class="form-horizontal" action="/score_entry" enctype="multipart/form-data" method="post" id="games_upload">
		<div class="form-group">
			<label for="team1" class="col-sm-2 control-label">Team 1:</label>
			<div class="col-sm-10">   
				<input type="text" name="team1" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="goals1" class="col-sm-2 control-label">Team 1 score:</label>
			<div class="col-sm-10">   
				<input type="text" name="goals1" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="goals2" class="col-sm-2 control-label">Team 2 Score:</label>
			<div class="col-sm-10">   
				<input type="text" name="goals2" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="team2" class="col-sm-2 control-label">Team 2:</label>
			<div class="col-sm-10">   
				<input type="text" name="team2" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="date" class="col-sm-2 control-label">Date:</label>
			<div class="col-sm-10">   
				<input type="text" name="date" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-offset-2 col-sm-10">
				<input class="btn btn-info" type="submit" value="upload" name="push_button">
			</div>
		</div>
	</form>

	<form class="form-horizontal" action="/log_entry" enctype="multipart/form-data" method="post" id="log_upload">
		<div class="form-group">
			<label for="squad" class="col-sm-2 control-label">Squad:</label>
			<div class="col-sm-10">   
				<input type="text" name="squad" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="wins" class="col-sm-2 control-label">Wins:</label>
			<div class="col-sm-10">   
				<input type="text" name="wins" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="draws" class="col-sm-2 control-label">Draws:</label>
			<div class="col-sm-10">   
				<input type="text" name="draws" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="losses" class="col-sm-2 control-label">Losses:</label>
			<div class="col-sm-10">   
				<input type="text" name="losses" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="points" class="col-sm-2 control-label">Points:</label>
			<div class="col-sm-10">   
				<input type="text" name="points" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-offset-2 col-sm-10">
				<input class="btn btn-info" type="submit" value="upload" name="push_button">
			</div>
		</div>
	</form>

</body>
</html>